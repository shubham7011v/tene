{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Flutter: Run (Development)",
            "type": "shell",
            "command": "flutter",
            "args": [
                "run",
                "--dart-define=ENV=dev"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false
            },
            "problemMatcher": []
        },
        {
            "label": "Flutter: Run (Production)",
            "type": "shell",
            "command": "flutter",
            "args": [
                "run",
                "--dart-define=ENV=prod"
            ],
            "group": "build",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false
            },
            "problemMatcher": []
        },
        {
